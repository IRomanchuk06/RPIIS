\noindent \textbf{\textit{key query'}}
\begin{itemize}
\item[$\Rightarrow$]\textit{first domain*:}

\textit{key query*}

\item[$\Rightarrow$]\textit{second domain*:}

\textit{key query}

\item[$\Rightarrow$]\textit{explanation*:}

[A key query '

is a search-verify query (from one

known element) that is executed from at least

one element and is not executed from at least

one element.]
\\
\end{itemize}

\noindent \textbf{\textit{element of key query '}}

\begin{itemize}

\item[$\Rightarrow$]\textit{first domain*:}

\textit{key query*}

\item[$\Rightarrow$]\textit{second domain*:}

\textit{element of key query}
\\
\end{itemize}

\noindent \textbf{\textit{minimal key query '}}

\begin{itemize}

 \item[\textbf{$\subset$} ] \textit{key query'}

\item[$\Rightarrow$]\textit{explanation*:}


[A minimal key query is a key query that finds

sc-subsets of the element sets found by all other

key queries that have the same domains of known

satisfiability and non-satisfiability elements.] \\

\end{itemize}

\noindent \textbf{\textit{element of minimal key query'}}

\begin{itemize}

\item[$\Rightarrow$]\textit{first domain*:}


\textit{minimal key query*}

\item[$\Rightarrow$]\textit{second domain*:}




\textit{element of minimal key query} \\

\\

\end{itemize}

\noindent \textbf{\textit{full semantic neighborhood of an element*}}

\begin{itemize}

\item[$\Rightarrow$]\textit{explanation*:}


[A full semantic neighborhood of an element*


is all elements found by minimal key queries

from this element (taking into account disjunctive

search and negation of search).]




%%\textit{element of key query}

\end{itemize}

