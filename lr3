\begin{itemize}
\item[$\Rightarrow$]\textit{first domain*:}

\textit{semantic continuous similarity of sc-structures*}

\item[$\Rightarrow$]\textit{second domain*:}

\textit{sc-relation of semantic continuous similarity by}

\textit{slot relations*}
\\
\end{itemize}
